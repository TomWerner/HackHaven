<h1>Contest Creation</h1>
<br>
<div class="panel panel-default">
        <div class="panel panel-heading">
            Contest Details:
        </div>
    <div class="panel panel-body">
    <%= form_for :contest, url: {action: "create"}, :class => "form-inline" do |f| %>
       <p>
            <%= f.label :contestname, 'Name of Contest' %><br>
            <%= f.text_field :contestname, :class => "form-control" %>
        </p>
        
        <p>
            <%= f.label :contestdate, 'Date' %><br>
            <%= f.date_select :contestdate, :class => "form-control" %>
        </p>
        
        <p>
            <%= f.submit 'Submit', :class => "btn btn-primary" %>
        </p>
        
        <% end %>
    </div>
</div>

